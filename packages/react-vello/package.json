{
  "name": "react-vello",
  "version": "0.0.4",
  "license": "MIT",
  "author": "Matthew Blode <m@blode.co>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mblode/react-vello.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json && node ./scripts/copy-wasm.cjs",
    "dev": "tsc --project tsconfig.json --watch",
    "prepack": "npm run build",
    "check-types": "tsc --project tsconfig.json --noEmit",
    "lint": "tsc --project tsconfig.json --noEmit",
    "test": "echo 'renderer not implemented yet'"
  },
  "dependencies": {
    "react-reconciler": "0.33.0"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-reconciler": "^0.32.3",
    "typescript": "~5.9.3"
  }
}
